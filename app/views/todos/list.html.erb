<h1>Pages Lista</h1>

<div class="row">
  <div class="col-md-6"><h2>Listar no completados</h2>
    <% @todos.each do |todo| %>
    <%if todo.completed == false %>
    <ul>
    <li><%= todo.description %></li>
  </ul>
    <% end %>
    <%end %>
  </div>

  <div class="col-md-6">  <h2>Listar completados</h2>
    <% @todos.each do |todo| %>
    <%if todo.completed == true %>
    <ul>
    <li><%= todo.description %></li>
    </ul>
    <% end %>
    <%end %>
  </div>
</div>
